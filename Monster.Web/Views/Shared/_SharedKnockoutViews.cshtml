@using Monster.Web.Plumbing
<script type="text/html" id="Execution-Logs">
    <div style="text-align:center; padding:15px; letter-spacing: 5px; font-weight:600; text-transform: uppercase;">
        @GlobalConfig.AppName - Execution Logs
    </div>

    <div style="overflow-y: scroll; height: 600px; text-align: left;">
        <div data-bind="if: $data.length">
            <table class="table table-striped">
                <tbody data-bind="foreach: $data">
                <tr>
                    <td>
                        <div style="font-size: 14px; letter-spacing: 1px; color: #777;">
                            <span data-bind="text: LogTime"></span> (UTC)
                        </div>
                        <div data-bind="text: Content"></div>
                    </td>
                </tr>
                </tbody>
            </table>
        </div>
    </div>
</script>



